@model DMSFinal.BasicData.ViewModels.AchievementTransformationVMs.AchievementTransformationVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
    <wt:row items-per-row="ItemsPerRowEnum.Two">
        <wt:display field="Entity.Department" />
        <wt:display field="Entity.JobNo" />
        <wt:display field="Entity.Name" />
        <wt:display field="Entity.Title1" />
        <wt:display field="Entity.Title2" />
        <wt:display field="Entity.SubjectName" />
        <wt:display field="Entity.CompanyName" />
        <wt:display field="Entity.Funds" />
        <wt:display field="Entity.StartTime" />
        <wt:display field="Entity.EndTime" />
        <wt:display field="Entity.Participants" />
        <wt:multiupload field="Entity.Evidence" upload-type="AllFiles" disabled="true"/>
        <wt:multiupload field="Entity.ApprovalDoc" upload-type="AllFiles" disabled="true"/>
        <wt:display field="Entity.Examine" />
        <wt:display field="Entity.Reason" />
    </wt:row>
    <wt:row align="AlignEnum.Right">
        <wt:closebutton />
    </wt:row>
</wt:form>
