@model DMSFinal.BasicData.ViewModels.VerticalSubjectVMs.VerticalSubjectVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
	<wt:quote>@Localizer["Sys.DeleteConfirm"]</wt:quote>
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:display field="Entity.Department" />
<wt:display field="Entity.JobNo" />
<wt:display field="Entity.Name" />
<wt:display field="Entity.Title1" />
<wt:display field="Entity.Title2" />
<wt:display field="Entity.SubjectName" />
<wt:display field="Entity.SubjectLevel" />
<wt:display field="Entity.SubjectSource" />
<wt:display field="Entity.SubjectNo" />
<wt:display field="Entity.Funds" />
<wt:display field="Entity.StartTime" />
<wt:display field="Entity.IfCose" />
<wt:display field="Entity.EndTime" />
<wt:display field="Entity.Rank" />
<wt:display field="Entity.IfOurSchool" />
<wt:display field="Entity.Examine" />
<wt:display field="Entity.Reason" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton theme=" ButtonThemeEnum.Warm" text="@Localizer["Sys.Delete"]"/>
        <wt:closebutton />
    </wt:row>
</wt:form>
