@model DMSFinal.ApprovedData.ViewModels.PaperVMs.PaperListVM
@inject IStringLocalizer<Program> Localizer;

<wt:searchpanel vm="@Model" reset-btn="true">
<wt:row items-per-row="ItemsPerRowEnum.Three">
<wt:combobox field="Searcher.Department" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.JobNo" />
<wt:textbox field="Searcher.Name" />
<wt:combobox field="Searcher.Title1" empty-text="@Localizer["Sys.All"]" />
<wt:combobox field="Searcher.Title2" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.PaperTitle" />
<wt:textbox field="Searcher.MagazineName" />
<wt:datetime field="Searcher.PublicTime" range="true" />
<wt:textbox field="Searcher.Info" />
<wt:combobox field="Searcher.Category" empty-text="@Localizer["Sys.All"]" />
<wt:combobox field="Searcher.IsSCI" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.SCINo" />
<wt:combobox field="Searcher.IsEI" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.EINo" />
<wt:combobox field="Searcher.IsISTP" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.ISTPNo" />
<wt:textbox field="Searcher.Num" />
<wt:textbox field="Searcher.ImpactFactor" />
<wt:combobox field="Searcher.Rank" empty-text="@Localizer["Sys.All"]" />
<wt:combobox field="Searcher.IsOurSchool" empty-text="@Localizer["Sys.All"]" />
@*<wt:combobox field="Searcher.Examine" empty-text="@Localizer["Sys.All"]" />
<wt:textbox field="Searcher.Reason" />*@
</wt:row>
</wt:searchpanel>
<wt:grid vm="@Model" url="/ApprovedData/Paper/Search"/>
