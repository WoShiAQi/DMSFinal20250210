@model DMSFinal.ApprovedData.ViewModels.AchievementTransformationVMs.AchievementTransformationVM
@inject IStringLocalizer<Program> Localizer;

<wt:form vm="@Model">
<wt:row items-per-row="ItemsPerRowEnum.Two">
<wt:combobox field="Entity.Department" />
<wt:textbox field="Entity.JobNo" />
<wt:textbox field="Entity.Name" />
<wt:combobox field="Entity.Title1" />
<wt:combobox field="Entity.Title2" />
<wt:textbox field="Entity.SubjectName" />
<wt:textbox field="Entity.CompanyName" />
<wt:textbox field="Entity.Funds" />
<wt:datetime field="Entity.StartTime" />
<wt:datetime field="Entity.EndTime" />
<wt:textbox field="Entity.Participants" />
<wt:combobox field="Entity.Examine" />
<wt:textbox field="Entity.Reason" />
</wt:row>
    <wt:hidden field="Entity.ID" />
    <wt:row align="AlignEnum.Right">
        <wt:submitbutton />
        <wt:closebutton />
    </wt:row>
</wt:form>
